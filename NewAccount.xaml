<Page x:Class="MFA.NewAccount"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MFA"
      mc:Ignorable="d" 
      Title="NewAccount" Height="400" Width="300" Background="LightGray">

    <Grid>
        <Button x:Name="QRcapture" Content="QRcode" HorizontalAlignment="Center" VerticalAlignment="Top" Height="30" Margin="0,240,0,0"  Width="80" Click="Capture"/>
        <TextBox x:Name="SecretKey" HorizontalAlignment="Right" VerticalAlignment="Top" Height="35" Margin="0,190,30,0" Width="240"/>
        <Button x:Name="Retour" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Height="30" Margin="40,40,0,0" Width="30" Click="Back"/>
    </Grid>
</Page>
